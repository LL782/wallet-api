{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "lib-es/**"],
      "inputs": ["src/**/*.ts"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "tsc": {
      "outputs": []
    },
    "wallet-api-website#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"],
      "inputs": ["src/**", "docs/**", "static/**", "*.js"]
    },
    "wallet-api-tools#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "wallet-api-tools#dev": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
